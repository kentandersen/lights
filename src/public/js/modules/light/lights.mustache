{{#lights}}
    <li data-light-id="{{id}}">
        {{name}} [{{status}}]
        <a class="on" href="#">on</a>
        <a class="off" href="#">off</a>

        {{#isDimmable}}
        <br />
             <input type="range" class="dimLevel" name="dimLevel" value="{{dimLevel}}" min="0" step="1" max="255">
        {{/isDimmable}}
    </li>
{{/lights}}